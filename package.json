{
  "name": "explore-docker-react",
  "version": "0.0.0",
  "description": "An example project to explore React apps using Docker",
  "scripts": {
    "build": "npm run start:clean",
    "start": "npm run start:create-react-app-example-02",
    "start:clean": "npm run start:create-react-app-example-02:clean",
    "stop": "npm run stop:create-react-app-example-02",
    "start:create-react-app": "cd examples/create-react-app && npm start",
    "start:create-react-app:clean": "cd examples/create-react-app && npm run build",
    "stop:create-react-app": "cd examples/create-react-app && npm run stop",
    "start:create-react-app-experimental": "cd examples/create-react-app && npm start",
    "start:create-react-app-experimental:clean": "cd examples/create-react-app && npm run build",
    "stop:create-react-app-experimental": "cd examples/create-react-app && npm run stop",
    "start:create-react-app-example-01": "cd examples/create-react-app-example-01 && npm start",
    "start:create-react-app-example-01:clean": "cd examples/create-react-app-example-01 && npm run build",
    "stop:create-react-app-example-01": "cd examples/create-react-app-example-01 && npm run stop",
    "start:create-react-app-example-02": "cd examples/create-react-app-example-02 && npm start",
    "start:create-react-app-example-02:clean": "cd examples/create-react-app-example-02 && npm run build",
    "stop:create-react-app-example-02": "cd examples/create-react-app-example-02 && npm run stop",
    "docker:nuke": "docker system prune -a && docker volume prune"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheRobBrennan/explore-docker-react.git"
  },
  "author": "Rob Brennan <rob@therobbrennan.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheRobBrennan/explore-docker-react/issues"
  },
  "homepage": "https://github.com/TheRobBrennan/explore-docker-react#readme"
}
